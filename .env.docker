# Database for Docker
POSTGRES_SERVER=db
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=rag_db

# Async URL cho FastAPI application
DATABASE_URL=postgresql+asyncpg://user:password@db:5432/rag_db

# Sync URL cho Alembic migrations
DATABASE_URL_SYNC=postgresql://user:password@db:5432/rag_db

# API Settings
API_V1_STR=/api/v1
PROJECT_NAME=RAG PDF Chatbot
VERSION=1.0.0

# File Upload
MAX_FILE_SIZE=52428800
UPLOAD_FOLDER=uploads

# LLM Settings
LLM_MODEL=qwen3:1.7b

# Logging
LOG_LEVEL=INFO